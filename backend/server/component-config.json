{
  "loopback-component-explorer": {
    "mountPath": "/explorer"
  },
  "loopback-component-winston": {
    "level": "debug",
    "transports": [
      {
        "type": "Console",
        "name": "console-all",
        "json": false,
        "timestamp": true
      },
      {
        "type": "File",
        "name": "verbose-file",
        "timestamp": true,
        "filename": "C:/dev/workspace/logs/shuru/log.log",
        "maxsize": 1000000,
        "maxFiles": 10,
        "logstash": true,
        "level": "error"
      }
    ]
  }
}
